package com.bean;

import java.util.List;

public class DescriotorDoc {
	// 分类描述总单ID
	private String sortdescdocid;
	// 疾病介绍
	private String diseasedesc;
	// 治疗方案
	private String treatmentplan;
	// 细单集合
	private List<DescriotorDtl> dtllist;

	public String getSortdescdocid() {
		return sortdescdocid;
	}

	public void setSortdescdocid(String sortdescdocid) {
		this.sortdescdocid = sortdescdocid;
	}

	public String getDiseasedesc() {
		return diseasedesc;
	}

	public void setDiseasedesc(String diseasedesc) {
		this.diseasedesc = diseasedesc;
	}

	public String getTreatmentplan() {
		return treatmentplan;
	}

	public void setTreatmentplan(String treatmentplan) {
		this.treatmentplan = treatmentplan;
	}

	public List<DescriotorDtl> getDtllist() {
		return dtllist;
	}

	public void setDtllist(List<DescriotorDtl> dtllist) {
		this.dtllist = dtllist;
	}

}
